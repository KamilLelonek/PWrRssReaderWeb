# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET		/								                            controllers.Application.index
GET     /feeds                                                      controllers.Application.feeds
GET     /feedsFrom/:lastUpdateTime                                  controllers.Application.feedsFrom(lastUpdateTime: Long)

GET     /getFeedsFromChannels/:lastUpdateTime/$id<(\d+\&)+>         controllers.Application.getFeedsFromChannels(lastUpdateTime: Long, id: String)
# example:  http://localhost/123456/8&9&10

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)